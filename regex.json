[
    {
        "name": "hive.security.authorization.createtable.group.grants",
        "regex": "|[a-z]+(,[a-z]+):(all|alter|update|create|drop|index|lock|select|show_database)(;[a-z]+(,[a-z]+):(all|alter|update|create|drop|index|lock|select|show_database))*"
    },
    {
        "name": "datanucleus.storeManagerType",
        "regex": "rdbms"
    },
    {
        "name": "hive.vectorized.input.format.supports.enabled",
        "regex": "decimal_64"
    },
    {
        "name": "hive.exec.stagingdir",
        "regex": "[.a-z][a-z]*(-[a-z]+)*"
    },
    {
        "name": "hive.llap.zk.registry.user",
        "regex": "[a-z]+"
    },
    {
        "name": "hive.execution.mode",
        "regex": "container|llap"
    },
    {
        "name": "hive.exec.default.partition.name",
        "regex": "__HIVE_DEFAULT_PARTITION__"
    },
    {
        "name": "hive.vectorized.adaptor.usage.mode",
        "regex": "none|chosen|all"
    },
    {
        "name": "hive.llap.daemon.queue.name",
        "regex": "llap_queue"
    },
    {
        "name": "hive.metastore.event.message.factory",
        "regex": "org\\.apache\\.hadoop\\.hive\\.metastore\\.messaging\\.json\\.(gzip\\.GzipJSONMessageEncoder|JSONMessageEncoder)"
    },
    {
        "name": "hive.server2.tez.session.lifetime",
        "regex": "-1|[1-9][0-9][0-9][0-9][0-9](ns|us|ms|s|m|h|d)"
    },
    {
        "name": "metastore.metadata.transformer.class",
        "regex": "org\\.apache\\.hadoop\\.hive\\.metastore\\.MetastoreDefaultTransformer"
    },
    {
        "name": "hive.materializedview.rewriting.strategy",
        "regex": "heuristic"
    },
    {
        "name": "hive.metastore.serdes.without.from.deserializer",
        "regex": "org\\.apache\\.iceberg\\.mr\\.hive\\.HiveIcebergSerDe"
    },
    {
        "name": "hive.service.metrics.codahale.reporter.classes",
        "regex": "org\\.apache\\.hadoop\\.hive\\.common\\.metrics\\.metrics2\\.JsonFileMetricsReporter, org\\.apache\\.hadoop\\.hive\\.common\\.metrics\\.metrics2\\.JmxMetricsReporter"
    },
    {
        "name": "hive.metastore.filter.hook",
        "regex": "org\\.apache\\.hadoop\\.hive\\.metastore\\.DefaultMetaStoreFilterHookImpl"
    },
    {
        "name": "fs.permissions.umask-mode",
        "regex": "[0-7][0-7][0-7]"
    },
    {
        "name": "mapreduce.map.log.level",
        "regex": "INFO|DEBUG|FATAL"
    },
    {
        "name": "hive.execution.engine",
        "regex": "mr|tez|spark"
    },
    {
        "name": "tez.queue.name",
        "regex": "default|test|tez_queue|tezq1|nondefault"
    },
    {
        "name": "hive.support.quoted.identifiers",
        "regex": "column|none"
    },
    {
        "name": "hive.metastore.server.thrift.transport.mode",
        "regex": "binary"
    },
    {
        "name": "hive.query.result.fileformat",
        "regex": "SequenceFile|TextFile|RCfile"
    },
    {
        "name": "datanucleus.plugin.pluginRegistryBundleCheck",
        "regex": "EXCEPTION|LOG|NONE"
    },
    {
        "name": "hive.blobstore.supported.schemes",
        "regex": "s3,s3a,s3n"
    },
    {
        "name": "hive.server2.logging.operation.level",
        "regex": "EXECUTION|NONE"
    },
    {
        "name": "hive.txn.manager",
        "regex": "org\\.apache\\.hadoop\\.hive\\.ql\\.lockmgr\\.DummyTxnManager|org\\.apache\\.hadoop\\.hive\\.ql\\.lockmgr\\.DbTxnManager"
    },
    {
        "name": "mapreduce.reduce.log.level",
        "regex": "INFO|DEBUG|FATAL"
    },
    {
        "name": "datanucleus.connectionPoolingType",
        "regex": "HikariCP|DBCP|dbcp2|C3P0|Proxool|BoneCP|TomCat"
    },
    {
        "name": "hive.cbo.fallback.strategy",
        "regex": "CONSERVATIVE"
    },
    {
        "name": "hive.ddl.output.format",
        "regex": "text"
    },
    {
        "name": "hive.driver.parallel.compilation.global.limit",
        "lower": "-1",
        "upper": "100"
    },
    {
        "name": "hive.vectorized.testing.reducer.batch.size",
        "lower": "-1",
        "upper": "10000"
    },
    {
        "name": "hive.server2.llap.concurrent.queries",
        "lower": "-1",
        "upper": "100"
    },
    {
        "name": "hive.tez.unordered.output.buffer.size.mb",
        "lower": "-1",
        "upper": "10000"
    },
    {
        "name": "hive.hashtable.initialCapacity",
        "lower": "0"
    }
]